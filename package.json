{
  "name": "starter-gatsby-postcss-opinionated",
  "private": false,
  "description": "A Gatsby + PostCSS bare-bone opinionated starter.",
  "version": "0.1.0",
  "author": "Mike Vo <info@miketvo.com>",
  "dependencies": {
    "gatsby": "^3.10.2",
    "gatsby-plugin-gatsby-cloud": "^2.10.1",
